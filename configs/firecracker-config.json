{
    "boot-source": {
        "kernel_image_path": "x86_64-vmlinux",
        "boot_args": "console=ttyS0 reboot=k panic=1 init=/walkio/init"
    },
    "drives": [
        {
            "drive_id": "rootfs",
            "path_on_host": "rootfs.ext4",
            "is_root_device": true,
            "is_read_only": true
        },
        {
            "drive_id": "appfs",
            "path_on_host": "app.ext4",
            "is_root_device": false,
            "is_read_only": true
        },
        {
            "drive_id": "state",
            "path_on_host": "state.ext4",
            "is_root_device": false,
            "is_read_only": false
        }
    ],
    "machine-config": {
        "vcpu_count": 1,
        "mem_size_mib": 64,
        "smt": false
    }
}
